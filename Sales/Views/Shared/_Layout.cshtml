
<!doctype html>
<html direction="rtl" dir="rtl" style="direction: rtl">
<!--begin::Head-->
<head>
	@* <title>@MyVariables.WebSiteTitle | @ViewData["Page"]</title> *@
	<title>@ViewData["Page"]</title>
	<meta charset="utf-8" />
	@* <meta name="description" content="@MyVariables.WebSiteContent" />
	<meta name="keywords" content="@MyVariables.WebSiteContent" /> *@
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<meta property="og:locale" content="en_US" />
	<meta property="og:type" content="article" />
	<meta property="og:title" content="@MyVariables.WebSiteTitle" />
	<meta property="og:url" content="@MyVariables.WebSitePath" />
	@* <meta property="og:site_name" content="@MyVariables.WebSiteContent" /> *@
	<link rel="canonical" href="@MyVariables.WebSitePath" />
	<link rel="shortcut icon" href="/favicon.ico" />
	<!--begin::Fonts(mandatory for all pages)-->
	<link rel="preconnect" href="https://fonts.googleapis.com">
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
	<link href="https://fonts.googleapis.com/css2?family=Tajawal:wght@200;300;400;500&display=swap" rel="stylesheet">
	<!--end::Fonts-->
	<!--begin::Vendor Stylesheets(used for this page only)-->
	@await RenderSectionAsync("CSS", required: false)
	<!--end::Vendor Stylesheets-->
	<!--begin::Global Stylesheets Bundle(mandatory for all pages)-->
	<link href="/assets/plugins/global/plugins.bundle.rtl.css" rel="stylesheet" type="text/css" />
	<link href="/assets/css/style.bundle.rtl.css" rel="stylesheet" type="text/css" />
	<!--end::Global Stylesheets Bundle-->
	<script>// Frame-busting to prevent site from being loaded within a frame without permission (click-jacking) if (window.top != window.self) { window.top.location.replace(window.self.location.href); }</script>
</head>
<!--end::Head-->
<!--begin::Body-->
<body id="kt_app_body" data-kt-app-header-fixed="true" data-kt-app-header-fixed-mobile="true" data-kt-app-aside-enabled="true" data-kt-app-aside-fixed="false" class="bg-gray-100">
	<!--begin::Theme mode setup on page load-->
	<script>var defaultThemeMode = "light"; var themeMode; if (document.documentElement) { if (document.documentElement.hasAttribute("data-bs-theme-mode")) { themeMode = document.documentElement.getAttribute("data-bs-theme-mode"); } else { if (localStorage.getItem("data-bs-theme") !== null) { themeMode = localStorage.getItem("data-bs-theme"); } else { themeMode = defaultThemeMode; } } if (themeMode === "system") { themeMode = window.matchMedia("(prefers-color-scheme: dark)").matches ? "dark" : "light"; } document.documentElement.setAttribute("data-bs-theme", themeMode); }</script>
	<!--end::Theme mode setup on page load-->
	<!--begin::App-->
	<div class="d-flex flex-column flex-root app-root" id="kt_app_root">
		<!--begin::Page-->
		<div class="app-page flex-column flex-column-fluid" id="kt_app_page">
			<!--begin::Header-->
			<div id="kt_app_header" class="app-header">
				<!--begin::Header logo-->
				<div class="app-header-logo d-flex align-items-center ps-lg-10 gap-4 gap-lg-6">
					<!--begin::Aside toggle-->
					<button id="kt_app_aside_toggle" class="btn btn-icon bg-gray-300 bg-opacity-75 btn-color-gray-700 btn-active-color-primary rounded-1 w-30px h-30px ms-n3 ms-lg-0">
						<i class="ki-outline ki-burger-menu-6 fs-3"></i>
					</button>
					<!--end::Aside toggle-->
					<!--begin::Logo image-->
					<a href="/Home">
						<img alt="Logo" src="~/images/school-logo-bg.png" class="h-50px d-sm-none d-inline theme-light-show" />
						<img alt="Logo" src="~/images/school-logo-bg.png" class="h-50px h-lg-55px theme-light-show d-none d-sm-inline" />
						<img alt="Logo" src="~/images/school-logo-bg.png" class="h-50px h-lg-55px theme-dark-show" />
					</a>
					<!--end::Logo image-->
				</div>
				<!--end::Header logo-->
				<!--begin::Header wrapper-->
				<div class="app-header-wrapper">
					<div class="app-container container-fluid">
						<!--begin::Navbar-->
						<div class="app-navbar flex-shrink-0">
							<!--begin::User menu-->
							@await Component.InvokeAsync("LoadLoggedData")
							<!--end::User menu-->
							<!--begin::Action-->
							<div class="app-navbar-item ms-1 ms-lg-3">

								<form asp-controller="Home" asp-action="SignOut" method="post">
									<button type="submit" class="btn btn-sm btn-outline btn-light-danger btn-flex btn-custom px-3"><i class="ki-outline ki-entrance-left fs-2 me-2"></i>تسجيل الخروج</button>
								</form>
							</div>
							<!--end::Action-->
						</div>
						<!--end::Navbar-->
					</div>
				</div>
			</div>
			<!--end::Header-->
			<!--begin::Wrapper-->
			<div class="app-wrapper d-flex" id="kt_app_wrapper">
				<!--begin::Main-->
				<div class="app-main flex-column flex-row-fluid" id="kt_app_main">
					<div id="divModelShared"></div>
					@RenderBody()
					<div class="mt-20"></div>
					<!--begin::Footer-->
					<div id="kt_app_footer" class="app-footer">
						<!--begin::Footer container-->
						<div class="app-container container-fluid d-flex flex-column flex-md-row flex-center flex-md-stack py-3 row">
							<div class="col-xl-4"></div>
							<div class="col-xl-4"></div>
							<div class="col-xl-4">
								<!--begin::Copyright-->
								<div class="text-dark order-2 order-md-1 fs-4">
									<span class="text-muted fw-semibold me-1">2025&copy;</span>
									<a href="http://www.alsahmsoft.com/" target="_blank" class="text-gray-800 text-hover-primary">برمجة وتصميم السهم سوفت</a>
								</div>
								<!--end::Copyright-->
							</div>

						</div>
						<!--end::Footer container-->
					</div>
					<!--end::Footer-->
				</div>
				<!--end:::Main-->
				<!--begin::aside-->
				<div id="kt_app_aside" class="app-aside flex-column" data-kt-drawer="true" data-kt-drawer-name="app-aside" data-kt-drawer-activate="true" data-kt-drawer-overlay="true" data-kt-drawer-width="auto" data-kt-drawer-direction="start" data-kt-drawer-toggle="#kt_app_aside_toggle">
					<!--begin::Aside wrapper-->
					<div id="kt_app_aside_wrapper" class="app-aside-wrapper hover-scroll-y my-4" data-kt-scroll="true" data-kt-scroll-activate="true" data-kt-scroll-height="auto" data-kt-scroll-offset="5px">
						<partial name="_navbar" />
					</div>
					<!--end::Aside wrapper-->
				</div>
				<!--end::aside-->
			</div>
			<!--end::Wrapper-->
		</div>
		<!--end::Page-->
	</div>
	<!--end::App-->
	<!--begin::Javascript-->
	<script>var hostUrl = "assets/";</script>
	<!--begin::Global Javascript Bundle(mandatory for all pages)-->
	<script src="/assets/plugins/global/plugins.bundle.js"></script>
	<script src="/assets/js/scripts.bundle.js"></script>
	<script src="/js/Shared.js"></script>
	<!--end::Global Javascript Bundle-->
	<!--begin::Custom Javascript(used for this page only)-->
	@await RenderSectionAsync("Scripts", required: false)
	<!--end::Custom Javascript-->
	<!--end::Javascript-->
	@RenderSection("Styles", required: false)

</body>
<!--end::Body-->
</html>